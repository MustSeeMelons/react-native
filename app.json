{
    "expo": {
        "name": "native-playground",
        "slug": "react-native",
        "privacy": "public",
        "sdkVersion": "35.0.0",
        "platforms": [
            "ios",
            "android",
            "web"
        ],
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "androidStatusBar": {
            "backgroundColor": "#00000000"
        },
        "androidNavigationBar": {
            "visible": true,
            "backgroundColor": "#000000",
            "barStyle": "light-content"
        },
        "android": {
            "package": "com.strautins.rn_weather"
        }
    }
}